@Library('jenkins-library') _
pipeline{

    agent any
    stages{

        stage('clone'){

            steps{
                script{

                    git 'https://github.com/veljkostevanic1996/Maven1.git' 'copy-Maven1'
                }
            }
        }

        stage('mvn install'){

            steps{

                script{
                   sh 'cd'
                   sh 'cd /home/vstevanic/Maven1'
                   sh 'mvn clean install'
                   //installMvn.install()
                }
            }
        }
    }
}