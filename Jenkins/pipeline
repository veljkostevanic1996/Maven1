@Library('jenkins-library') _
pipeline{

    agent any
    stages{

        stage('mvn install'){

            steps{

                withMaven{
                   sh 'cd'
                   sh 'sudo /home/vstevanic/Maven1'
                   sh 'mvn install'
                   //installMvn.install()
                   //echo installMvn.install()
                }
            }
        }
    }
}