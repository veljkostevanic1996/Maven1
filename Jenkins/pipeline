@Library('jenkins-library') _
pipeline{

    agent any
    stages{

        stage('clone'){

            steps{
                    git branch: 'master',
                    url: 'https://github.com/veljkostevanic1996/Maven1.git'

            }
        }

        stage('mvn install'){

            steps{

                script{
                   sh 'cd /home/vstevanic/Maven1'
                   installMvn.install()
                }
            }
        }
    }
}