@Library('jenkins-library') _
pipeline{

    agent any
    stages{

        stage('clone'){

            steps{

                sh git clone 'https://veljkostevanic1996:Veljkostevanic123/veljkostevanic1996/Maven1.git' as 'copy-Maven1'
            }
        }
        stage('mvn install'){

            steps{

                script{
                   sh 'cd'
                   sh 'cd /home/vstevanic/Maven1'
                   sh 'mvn clean install'
                   //installMvn.install()
                }
            }
        }
    }
}